asw_aba
	TMP = read_pc();
	TMP = set_h(TMP, read_pc());
	TMP3 = read(TMP);
	TMP3 = set_h(TMP3, read(TMP+1));
	P &= ~(F_C|F_N|F_Z);
	if(TMP3 & 0x8000)
		P |= F_C;
	TMP3 <<= 1;
	if(!TMP3)
		P |= F_Z;
	else if(TMP3 & 0x8000)
		P |= F_N;
	write(TMP, TMP3);
	write(TMP, TMP3 >> 8);
	prefetch();

