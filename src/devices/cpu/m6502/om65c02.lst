# m65c02 opcodes

adc_c_aba
	TMP = read_pc();
	TMP = set_h(TMP, read_pc());
	TMP = read(TMP);
	do_adc(TMP);
	if(P & F_D) {
		read_pc_noinc();
		set_nz(A);
	}
	prefetch();

adc_c_abx
	TMP = read_pc();
	TMP = set_h(TMP, read_pc());
	if(page_changing(TMP, X)) {
		read(set_l(TMP, TMP+X));
	}
	TMP += X;
	TMP = read(TMP);
	do_adc(TMP);
	if(P & F_D) {
		read_pc_noinc();
		set_nz(A);
	}
	prefetch();

