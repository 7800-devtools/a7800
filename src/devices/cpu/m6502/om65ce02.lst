brk_ce_imp
	if(irq_taken) {
		read_pc_noinc();
	} else {
		read_pc();
	}
	write(SP, PC >> 8);
	dec_SP_ce();
	write(SP, PC);
	dec_SP_ce();
	write(SP, irq_taken || nmi_state ? P & ~F_B : P);
	dec_SP_ce();
	if(nmi_state) {
		PC = read_arg(0xfffa);
		PC = set_h(PC, read_arg(0xfffb));
		nmi_state = false;
	} else {
		PC = read_arg(0xfffe);
		PC = set_h(PC, read_arg(0xffff));
	}
	irq_taken = false;
	P = (P | F_I) & ~F_D; // Do *not* move after the prefetch
	prefetch();
	inst_state = -1;

