bbr_zpb
	// Access pattern uncertain
	TMP = read_pc();
	TMP2 = read(TMP);
	TMP = read_pc();
	read_pc_noinc();
	if(!(TMP2 & (1 << ((inst_state >> 4) & 7)))) {
		PC += int8_t(TMP);
	}
	prefetch();

